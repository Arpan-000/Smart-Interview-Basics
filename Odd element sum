#include <bits/stdc++.h>
using namespace std;

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int n;
    scanf("%d",&n);  
    int a[n],sum =0;
    for(int i =0;i<n;i++) 
     { 
          scanf("%d",&a[i]); 
          if(a[i]%2!=0) 
           sum +=a[i];
     } 
 printf("%d",sum);
     return 0;
}
