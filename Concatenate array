#include <stdio.h>
#include <stdlib.h>

int main() {
int i,n;
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
  scanf("%d",&n);  
  int a[n*2];
  for(i=0;i<n;i++){  
      int num ;
       scanf("%d",&num);
       a[i] = num; 
       a[i+n]= num;
  } 
  for(i=0;i<2 * n;i++){ 
     printf("%d ", a[i]);
  }
  printf("\n");
    return 0;
}
