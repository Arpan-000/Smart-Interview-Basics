#include <bits/stdc++.h>
using namespace std;

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int n ,i,k=1,j; scanf("%d",&n); 
     int a[n]; 
      for(i=1;i<=n;i++) 
       {  for(j=1;j<=i;j++) 
            { printf("%d ",k); 
             k++;
       } 
       printf("\n") ; 
       }

    return 0;
}
